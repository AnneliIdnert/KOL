<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.idnert.kol_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <uses-permission android:name="android.permission.INTERNET" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
        <activity
            android:name=".AddExercis"
            android:label="@string/title_activity_add__exercis" />
        <activity android:name=".CardViewActivity" />
        <activity
            android:name=".RecyclerViewActivity"
            android:label="@string/showExercis" />
        <activity
            android:name=".Exercis_details"
            android:label="@string/title_activity_exercis_details" />
        <activity
            android:name=".PersonalPrefActivity"
            android:label="@string/title_activity_personal_pref" />
        <activity
            android:name=".StatisticActivity"
            android:label="@string/title_activity_statistics" />
        <!--
             ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".Star"
            android:label="@string/title_activity_star" />
        <activity
            android:name=".Point"
            android:label="@string/title_activity_point"></activity>
    </application>

</manifest>
