<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.idnert.kol_app" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <activity
                    android:name=".LayoutPersonalPref"
                    android:label="@string/personalPref" >
                </activity>

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddExercis"
            android:label="@string/title_activity_add__exercis" >
        </activity>
        <activity android:name=".CardViewActivity" >
        </activity>
        <activity
            android:name=".RecyclerViewActivity"
            android:label="@string/showExercis" >
        </activity>
        <activity
            android:name=".Exercis_details"
            android:label="@string/title_activity_exercis_details" >
        </activity>
        <activity
            android:name=".PersonalPrefActivity"
            android:label="@string/title_activity_personal_pref" >
        </activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
